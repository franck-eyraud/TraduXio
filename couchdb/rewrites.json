[{
  "from": "works/concordance",
  "to": "_list/concordance/concordance",
  "query": {
    "include_docs": "true",
    "stale":"update_after"
  }
},{
  "from": "languages",
  "to": "_view/languages",
  "query": {
    "group": "true",
    "stale":"update_after"
  }
},{
  "from": "works/version/:id",
  "to": "_update/version/:id"
},{
  "from": "works/copyright",
  "to": "_show/copyright"
},{
  "from": "works",
  "to": "_list/works/works",
  "query": {"stale":"update_after"}
},{
  "from": "shared/*",
  "to": "*"
},{
  "from": "robots.txt",
  "to": "robots.txt"
},{
  "from": "works/:id",
  "to": "_show/work/:id",
  "method": "GET"
},{
  "from": "works/work",
  "to": "_update/work",
  "method": "POST"
},{
  "from": "works/work/:id",
  "to": "_update/work/:id"
},{
  "from": "works/work/:id/:version",
  "to": "_update/work/:id"
},{
  "from": "works/license/:id/:version",
  "to": "_show/license/:id",
  "method": "GET"
},{
  "from": "works/:id/print/:version",
  "to": "_show/text/:id",
  "method": "GET"
},{
  "from": "glossary/:id",
  "to": "_update/glossary/:id",
  "method": "POST"
},{
  "from": "works/work/:id/glossary/:src_language/:src_sentence/:target_language",
  "to": "_update/glossary/:id"
},{
  "from": "works/:id/presence",
  "to": "_update/presence/:id"
},{
  "from": "",
  "to": "_show/welcome"
},{
  "from":"works/:id/chat",
  "method":"POST",
  "to":"_update/chat/:id"
},{
  "from":"works/:id/activity",
  "to":"_show/doc_activity/:id"
},{
  "from":"works/:id/changes/:seq",
  "to":"../../_changes",
  "query":{"filter":"traduxio/docid","feed":"longpoll","since":":seq"}
}]
